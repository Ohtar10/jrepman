<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>$pagetitle</title>
    <link rel="stylesheet" href="/css/bootstrap/bootstrap.min.css">
</head>
<body>

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6" align="center">
                <h1>$pagetitle</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <h3>Available Reports</h3>
                <table class="table table-stripped">
                        <tr>
                            <th>Report Name</th>
                            <th>Actions</th>
                        </tr>
                    #foreach($i in $reportlist)
                        <tr>
                            <td>$i</td>
                            <td><button class="btn btn-default">View Conf</button></td>
                        </tr>
                    #end
                </table>
            </div>
        </div>
    </div>

    <script src="/js/jquery/jquery.min.js"/>
    <script src="/js/bootstrap/bootstrap.min.js"/>
</body>

</html>